{
  "kind": "collectionType",
  "collectionName": "catalogues",
  "info": {
    "singularName": "catalogue",
    "pluralName": "catalogues",
    "displayName": "Catalogue"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "file": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "catalogue"
    }
  }
}
