{
  "kind": "collectionType",
  "collectionName": "catalogues",
  "info": {
    "singularName": "catalogue",
    "pluralName": "catalogues",
    "displayName": "Catalogue",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "file": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images", "files", "videos", "audios"]
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "catalogue"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "description": {
      "type": "string"
    },
    "KeyFeatures": {
      "type": "component",
      "repeatable": true,
      "component": "common.key-features"
    }
  }
}
